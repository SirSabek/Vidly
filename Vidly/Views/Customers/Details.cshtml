@model Customer

@{
    ViewBag.Title = "title";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>
<ul>
    <li>@Model.MembershipType.Name</li>
    @if(Model.BirthDate != null)
    {
        <li>@Model.BirthDate.Value.ToString("dd MMM yyyy")</li>
    }
    else
    {
        <li>Birthdate not set</li>
    }
</ul>

